<div id="header" class="tryIndex">
	<div role="navigation" class="navbar navbar-inverse" style="margin:0px;border-radius:0px;border:none;background-color:rgb(0,0,0);">
		<div class="row">
			<div class="col-lg-6">
				<div class="navbar-header">
					<div class="row mainBox">
						<div class="col-lg-1">
						</div>
						<button type="button" data-toggle="collapse" data-target=".navbar-collapse" class="navbar-toggle">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
						<div class="col-lg-11">
							<a href="/" class="" style="width:100%;height:auto;padding:0px;">
								<img alt="Rediscovery Cruises" src="../images/rc-logo.png" style="width:20%;height:auto;margin-bottom: 5px; margin-top: 5px;  margin-left: -5px;" />
							</a>
						</div>
					</div>
				</div>
			</div>
			<div class="col-lg-6">
				<div class="row">
					<div class="col-lg-11">
						<div class="collapse navbar-collapse" style="padding-right: 15px;padding-top:15px">
							<ul class="nav navbar-nav navbar-right">
								{{# each navLinks}}
									<li {{#ifeq ../section key}} class="active" {{else}} {{/ifeq}} style="font-family:Satisfy-Regular;font-size:22px;padding:20px;">
										<a href="{{ href }}" style="outline:none;color:rgb(235,235,235);background-color:rgba(0,0,0,0);">{{ label }}</a>
									</li>
									{{/each}}
									{{# each nav}}
										<li {{#ifeq ../section key}} class="active" {{else}} {{/ifeq}} style="font-family:Satisfy-Regular;font-size:20px;padding:25px;">
											<a href="{{ href }}" style="padding:5px;border-style: solid;border-width:2px;border-color:white;font-weight:normal;outline:none;color:white;background-color:rgba(0,0,0,0);">{{ label }}</a>
										</li>
										{{/each}}
							</ul>
						</div>
					</div>
					<div class="col-lg-1">
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- </div> -->
</div>
